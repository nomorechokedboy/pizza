{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**"],
			"env": ["TAURI_DEBUG", "CI"],
			"cache": false
		},
		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false
		},
		"test": {
			"outputs": []
		},
		"coverage": {
			"outputs": ["coverage/**"]
		}
	}
}
